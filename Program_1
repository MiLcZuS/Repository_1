string=str(input()

string="".join(c.lower() for c in string if c.isalpha() or c.isspace())
c
words=string.split()
filtr=[]

for word in words:
    if len(word)>1:
        filtr.append(word)

string=" ".join(filtr)

word_count={}
words=string.split()

for word in words:
    if word in word_count:
        word_count[word]=word_count[word]+1
    else:
        word_count[word]=1

naj_slowo=[]
naj_ilosc=0

for slowo,ilosc in word_count.items():
    if ilosc>naj_ilosc and word not in naj_slowo:
        naj_slowo=slowo
        naj_ilosc=ilosc
    

print(f"{naj_slowo}-{naj_ilosc}")
